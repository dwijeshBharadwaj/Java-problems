class Main {
    public static void main(String[] args){
        int year = 2000;
        if(((year%4==0) && (year%100!=0)) ||(year%400==0)){   // divisible by 4 but not 100 but by 400
            System.out.println("The year is a leap");
        }else{
            System.out.println("The year is not leap");
        }

    }
    
  }
