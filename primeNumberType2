import java.io.*;
import java.util.*;
class Main {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int i;
        int j;
        for(i=2;i<=N;i++){
            for ( j=2;j<=i-1;j++){
                if(i%j==0){
                    break;
                }
            }
            if(i==j){
                System.out.println(i);

            }
        }
    }   
}
